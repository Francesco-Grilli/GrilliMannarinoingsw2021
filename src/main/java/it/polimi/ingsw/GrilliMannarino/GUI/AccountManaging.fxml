   <Label fx:id="accountLabel" layoutX="208.0" layoutY="70.0" text="Create a New Account">
      <font>
         <Font size="20.0"/>
      </font>
   </Label>
   <TextField fx:id="usernameField" layoutX="231.0" layoutY="161.0" promptText="Username"/>
   <TextField fx:id="passwordField" layoutX="231.0" layoutY="211.0" promptText="Password"/>
   <TextField fx:id="confirmPasswordField" layoutX="231.0" layoutY="245.0" promptText="Confirm Password"/>
   <Button fx:id="accountButton" layoutX="275.0" layoutY="298.0" mnemonicParsing="false" text="Confirm"/>
</AnchorPane>
